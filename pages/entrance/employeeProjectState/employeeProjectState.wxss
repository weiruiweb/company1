page{
	min-height:100%;
	width:100%;
	background:#f2f2f2;
}
.state{
	border-left: solid 4rpx #21c3d4;
	position: relative;
}
.state_cont{
	height:200rpx;
}
.state_left{
	width:2.6%;
	height:100%;
	position:relative;
}
.state_right{
	width:97.4%;
	height:100%;
}
.state_right_box{
	width:94%;
	padding:0 3%;
	height:184rpx;
	background:#fff;
	border-radius: 20rpx;
}
.circle{
	width:16rpx;
	height:16rpx;
	background:#21c3d4;
	border-radius: 50%;
	position: absolute;
	left:-8rpx;
}
.state_top {
	height: 50%;
}
.state_bottom{
	height:40%;
	display: flex;
}
.next_page{
	text-align: right;
}
.next_page image{
	width:12rpx;
	height:20rpx;
}
.state_text{
	height:100%;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	word-break: break-all;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2; 
}
.date{
	height:90rpx;
	border-top:solid 1px #e5e5e5;
	border-bottom:solid 1px #e5e5e5;
	display:flex;
	align-items: center;
  	justify-content: center;
  	color:#666;
  	background:#fff;
}
.date_left{
	height:70rpx;
}
.date_item{
	height:70rpx;
	border-right:solid 1px #e5e5e5;
	width:50%;
	text-align:center;
	display:flex;
	align-items: center;
  	justify-content: center;
}
.date_item .text{
	height:50%;
}
.date_item image{
	width:17rpx;
	height:10rpx;
	margin-left:10rpx;
}
.clientList{padding:40rpx 22rpx;background:#fff;border-bottom:5rpx solid #f2f2f2}
.clientList image{width:124rpx;height:124rpx;}
.clientList_center{width:290rpx;}
.clientList_right{width:225rpx;line-height:63px}
.clientName{
  font-size:28rpx;
  color:#2e2e2ew;
}
.clientType{
  color:#666;
  font-size:26rpx;
  margin-bottom:10rpx;
}
.clientName text{float:left;margin-right:26rpx;}
.clientName image{
  width:23rpx;
  height:23rpx;
}
.clientPen{
  width:80rpx;
  height:40rpx;
  border-radius:30rpx;
  background:#f11f20;
  text-align:center;
  line-height:40rpx;
  color:#fff;
  font-size:22rpx;
  margin-bottom:20rpx;
}
.clientPhone{
  color:#666;
  font-size:24rpx;
  margin:15rpx 0rpx;
  }
.clientTime{
  color:#ddd;
  font-size:22rpx;

}
.User_cont{
	background:#f2f2f2;
	width:100%;
	width:50%;
	height:140rpx;
	text-align:center;
}
.user_img{
	width:100rpx;
	height:100rpx;
	overflow:hidden;
	border-radius:50%;
	margin-right:24rpx;
}
.user_img image{
	width:100%;
	height:100%;
	margin:0 auto;
}
.user_infor{
	text-align:left;
}