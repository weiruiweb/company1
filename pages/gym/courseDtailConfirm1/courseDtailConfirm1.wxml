        </view>订单确认</view>
    </view>
    <view style="width:100%;background:#262626;border-radius: 10rpx;margin-top: 20rpx;">
        <view class="w710">
            <view class="infor_list flexRowBetween">
                <view class="infor_left">
                    年卡:
                </view>
                <view class="infor_center">
