                        <view style="width:60rpx;height: 60rpx;{{img}};background-position:-60px 0;"></view>
                    </view>
                    <view class="number">1</view>
                    <view class="add">
                        <view style="width:60rpx;height: 60rpx;{{img}};background-position:0px 0;"></view>
                    </view>
                </view>
            </view>
             <view class="infor_list flexRowBetween"  bindtap="intoPath" data-path="/pages/gym/userDiscount/userDiscount">
