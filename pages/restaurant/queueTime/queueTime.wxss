page{
	width:100%;
	height:100vh;
	background:#f2f2f2;
}
.input_before{
	width:30%;
	font-size:32rpx;
	color:#666;
}
.queueTime{
	margin-top:20rpx;
}
.queue_list{
	padding:0rpx 5.5%;
	width:89%;
	height:80rpx;
	margin-bottom:20rpx;
	background:#fff;
	border-radius:10rpx;
	font-size:32rpx;
	color:#666;
}
.queue_input{
	width:70%;
}
.queue_input text{
	color:#f2665f;
}
.input_before image{
	width:11rpx;
	height:20rpx;
}
.discout{
	margin-top:20rpx;
}
.discount_bg{
	position:relative;
}
.discout_img image{
	width:100%;
	height:147rpx;
}
.discount_money{
	position:absolute;
	top:0px;
	height:147rpx;
	width:100%;
}
.discout_left{
	width:50%;
	height:147rpx;
	line-height:147rpx;
	font-size:80rpx;
	color:#fff;
	text-align:center;
}
.discout_right{
	width:50%;
	height:100rpx;
	text-align:center;
}
.discount_top{
	color:#fff;
	font-size:28rpx;
	height:60%;
}
.discount_bottom{
	width:110rpx;
	height:34rpx;
	border-radius:20rpx;
	background:#fffae3;
	color:#f2665f;
	font-size:26rpx;
	line-height:34rpx;
	text-align:center;
	margin:0px auto;

}
.discount_item{
	width:48%;
}
.discount_space{
	width:4%;
}
.queqe_btn{
	width:269rpx;
	height:269rpx;
	margin:160rpx auto 0;
	position:relative;
}
.queqe_btn image{
	width:259rpx;
	height:269rpx;
}
.queqe_txt{
	width:259rpx;
	height:269rpx;
	position:absolute;
	line-height:269rpx;
	text-align:center;
	font-size:24rpx;
	color:#fff;
	top:0;
	left:0;
}

